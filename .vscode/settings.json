{
	"better-comments.highlightPlainText": true,
	"breadcrumbs.enabled": true,
	"breadcrumbs.icons": false,
	"dart.closingLabels": false,
	"dart.debugExternalLibraries": false,
	"dart.debugSdkLibraries": false,
	"dart.enableCompletionCommitCharacters": true,
	"dart.flutterAndroidX": true,
	"dart.flutterHotRestartOnSave": true,
	"dart.flutterStructuredErrors": true,
	"dart.lineLength": 300,
	"dart.previewBuildRunnerTasks": true,
	"dart.previewFlutterUiGuides": true,
	"dart.previewFlutterUiGuidesCustomTracking": true,
	"dart.previewToStringInDebugViews": true,
	"dart.showIgnoreQuickFixes": true,
	"dart.showTodos": false,
	"debug.console.fontFamily": "FiraCode-Regular",
	"debug.console.fontSize": 12,
	"editor.detectIndentation": false,
	"editor.fontFamily": "FiraCode-Regular",
	"editor.fontLigatures": true,
	"editor.formatOnSave": true,
	"editor.insertSpaces": false,
	"editor.minimap.maxColumn": 200,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.showSlider": "always",
	"editor.tabSize": 3,
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"java.configuration.updateBuildConfiguration": "automatic",
	"markdown.preview.fontSize": 12,
	"terminal.integrated.fontSize": 12,
	"todo-tree.general.statusBar": "total",
	"workbench.colorTheme": "Cobalt2",
	"workbench.iconTheme": "material-icon-theme",
	"files.exclude": {
		".flutter-plugins": true,
		".metadata": true,
		".packages": true,
		"humblepos.iml": true,
		"pubspec.lock": true,
		"package-lock.json": true,
		"node_modules": true,
		"test": true,
		"web": true,
		".vscode": false,
		".idea": true,
		"build": true,
		".gitignore": false,
		".gitattributes": false,
		"README.md": true,
		"functions/node_modules": true,
		"functions/*.json": true,
		"functions/.gitignore": true,
		".firebaserc": true,
		"firebase.json": true,
		".dart_tool": true,
		".favorites.json": true,
		".flutter-plugins-dependencies": true
	},
	"macros": {
		"onSave": [
			"workbench.debug.panel.action.clearReplAction",
			"dart-import.fix",
			"editor.action.organizeImports",
			"workbench.action.files.save",
			"workbench.action.terminal.clear",
			"flutter.hotRestart"
		]
	},
	"editor.guides.indentation": true
}